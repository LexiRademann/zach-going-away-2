{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "includeFiles": [
    "app.py",
    "templates/**",
    "static/**",
    "css/**",
    "js/**",
    "images/*.jpg",
    "images/*.png"
  ]
}